{
  "mode": "testops",
  "fallback": "report",
  "debug": false,
  "environment": "local",
  "captureLogs": false,
  "report": {
    "driver": "local",
    "connection": {
      "local": {
        "path": "./build/qase-report",
        "format": "json"
      }
    }
  },
  "testops": {
    "api": {
      "token": "\u003ctoken\u003e",
      "host": "qase.io"
    },
    "run": {
      "title": "Regress run",
      "description": "Regress run description",
      "complete": true,
      "tags": [
        "tag1",
        "tag2"
      ],
      "configurations": {
        "values": [
          {
            "name": "browser",
            "value": "chrome"
          },
          {
            "name": "environment",
            "value": "staging"
          }
        ],
        "createIfNotExists": true
      }
    },
    "defect": false,
    "project": "\u003cproject_code\u003e",
    "batch": {
      "size": 100
    }
  }
}