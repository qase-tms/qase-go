{
  "mode": "testops",
  "fallback": "report",
  "debug": true,
  "environment": "staging",
  "captureLogs": true,
  "logging": {
    "logToConsole": true,
    "logToFile": true,
    "logDir": "./logs",
    "logFileName": "qase-staging.log",
    "logLevel": "DEBUG"
  },
  "report": {
    "driver": "local",
    "connection": {
      "local": {
        "path": "./build/qase-report",
        "format": "json"
      }
    }
  },
  "testops": {
    "api": {
      "token": "<your-api-token>",
      "host": "qase.io"
    },
    "run": {
      "title": "Automated Test Run",
      "description": "Test run in staging environment",
      "complete": true,
      "tags": ["automated", "staging", "regression"],
      "configurations": {
        "values": [
          {
            "name": "browser",
            "value": "chrome"
          },
          {
            "name": "environment",
            "value": "staging"
          }
        ],
        "createIfNotExists": true
      }
    },
    "defect": false,
    "project": "<your-project-code>",
    "batch": {
      "size": 100
    }
  }
}
