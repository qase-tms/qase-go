{
  "mode": "report",
  "fallback": "off",
  "debug": true,
  "environment": "demo",
  "captureLogs": true,
  "logging": {
    "logToConsole": true,
    "logToFile": true,
    "logDir": "./logs",
    "logFileName": "qase-demo.log",
    "logLevel": "DEBUG"
  },
  "report": {
    "driver": "local",
    "connection": {
      "local": {
        "path": "./build/qase-report",
        "format": "json"
      }
    }
  },
  "testops": {
    "api": {
      "token": "<your-api-token>",
      "host": "qase.io"
    },
    "run": {
      "title": "Multi-Package Demo Test Run",
      "description": "Demo test run with global initialization across multiple packages",
      "complete": true,
      "tags": ["demo", "multi-package", "global-init"],
      "configurations": {
        "values": [
          {
            "name": "browser",
            "value": "chrome"
          },
          {
            "name": "environment",
            "value": "demo"
          }
        ],
        "createIfNotExists": true
      }
    },
    "defect": false,
    "project": "<your-project-code>",
    "batch": {
      "size": 100
    }
  }
}
